    "@types/node": "13.11.1",
    "@types/supertest": "2.0.8",
    "@types/webtorrent": "0.107.2",
    "husky": "4.2.5",
    "prettier": "2.0.4",
    "supertest": "4.0.2",
    "ts-jest": "25.3.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.2",
    "tsc-watch": "4.2.3",
