{
  "name": "@pct-org/graphql-api",
  "version": "0.0.1",
  "description": "GraphQL API for the PCT environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/pct-org/graphql-api.git"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "src/"
  ],
  "main": "./dist/src/main.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:watch": "rm -rf dist && webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "postinstall": "yarn build",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "node dist/src/main.js",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev:watch": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:watch": "node ./dist/server.js"
  },
  "dependencies": {
    "@hapi/joi": "^16.0.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.5.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-fastify": "^6.6.7",
    "@nestjs/serve-static": "^1.0.1",
    "@pct-org/mongo-models": "^0.10.2",
    "apollo-server-fastify": "^2.9.3",
    "dotenv": "^8.1.0",
    "fastify-static": "^2.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^14.5.4",
    "graphql-subscriptions": "^1.1.0",
    "mongoose": "^5.7.0",
    "p-map": "^3.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.0",
    "rxjs": "^6.3.3",
    "type-graphql": "^0.17.5",
    "webtorrent-hybrid": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@nestjs/testing": "^6.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "4.16.1",
    "@types/hapi__joi": "^15.0.4",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "@types/webtorrent": "^0.107.0",
    "husky": "^3.0.5",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.1.0",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "webpack": "^4.40.0",
    "webpack-cli": "^3.3.8",
    "webpack-node-externals": "^1.7.2"
  }
}
